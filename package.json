{
  "name": "@bjoerge/immerge",
  "version": "1.0.0",
  "description": "Immutable shallow merge of plain JavaScript objects, maintaining referential equality when possible",
  "main": "immerge.js",
  "scripts": {
    "test": "jest && tsc --noEmit",
    "prepublish": "npm run build",
    "postpublish": "npm run clean",
    "build": "tsc --declaration",
    "clean": "rm -f immerge.d.ts immerge.js",
    "bench": "npm run build && node bench/runAll.js && npm run clean"
  },
  "keywords": [
    "merge",
    "immutable",
    "immerge",
    "referential equality"
  ],
  "author": "Bjørge Næss <bjoerge@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "benchmark": "^2.1.4",
    "jest": "^24.8.0",
    "prettier": "1.18.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.2.0",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.1"
  },
  "directories": {
    "test": "test"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bjoerge/immerge.git"
  },
  "bugs": {
    "url": "https://github.com/bjoerge/immerge/issues"
  },
  "homepage": "https://github.com/bjoerge/immerge#readme"
}
